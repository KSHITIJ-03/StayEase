<% layout('layout/boilerplate') %>

<h1>Edit</h1>

<form action="/houses/<%=house._id%>?_method=put"  method="POST">

    <label for="Name">Name</label>
    <input type="text" name="house[name]" id="Name" value="<%= house.name %>">
    
    <br>
    <label for="Address">Address</label>
    <input type="text" name="house[address]" id="Address" value="<%= house.address %>">
    
    <br>

    <label for="Price">Price</label>
    <input type="number" name="house[price]" id="Price" value="<%= house.price %>">
    
    <br>
    <label for="Availability">Availability</label>
    <input type="number" name="house[availability]" id="Availability" value="<%= house.availability %>">
    
    <br>
    <label for="description">description</label>
    <input type="text" name="house[description]" id="description" value="<%= house.description %>">
    

    <button>Submit</button>
    

    

</form>