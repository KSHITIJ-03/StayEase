<% layout('layout/boilerplate') %>

<h1>NEW</h1>

<form action="/houses" method="POST">

    <label for="Name">Name</label>
    <input type="text" name="house[name]" id="Name">
    
    <br>
    <label for="Address">Address</label>
    <input type="text" name="house[address]" id="Address">
    
    <br>

    <label for="Price">Price</label>
    <input type="number" name="house[price]" id="Price">
    
    <br>
    <label for="Availability">Availability</label>
    <input type="number" name="house[availability]" id="Availability">
    
    <br>
    <label for="description">description</label>
    <input type="text" name="house[description]" id="description">
    

    <button>Submit</button>

</form>